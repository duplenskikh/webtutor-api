{
  "compilerOptions": {
    "target": "ES5",
    "module": "Preserve",
    "noLib": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": false,
    "typeRoots": [
      "node_modules/@wshcmx/types/lib",
      "node_modules/@wshcmx/types/lib/xml"
    ],
    "noImplicitAny": true,
    "allowJs": true,
    "outDir": "build",
    "isolatedModules": true,
    "moduleResolution": "node",
    "strict": false,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "emitBOM": true
  },
  "include": [
    "src/**/*"
  ]
}
